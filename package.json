{
  "name": "odata-v4-server",
  "version": "0.1.36",
  "description": "OData V4 Server",
  "main": "build/lib/index.js",
  "typings": "build/lib/index",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "npm run tsc",
    "pretsc": "npm run lint",
    "tsc": "tsc",
    "prewatch": "rimraf build",
    "watch": "npm-watch",
    "es6": "copyfiles -u 1 src/**/*.js build",
    "lint": "tslint src/lib/**/*.ts -t verbose --force > .tslint",
    "pretest": "npm run build",
    "test": "mocha build/test/**/*.spec.js --reporter mochawesome --reporter-options reportDir=report,reportName=odata-v4-server,reportTitle=\"OData V4 Server\"",
    "precoverage": "npm run build",
    "coverage": "rm -rf coverage && node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha build/test/**/*.spec.js --config=.istanbul.yaml -x config/**/*.js && node node_modules/remap-istanbul/bin/remap-istanbul -i coverage/coverage.json -o coverage/html-report -t html",
    "prepublish": "npm run test",
    "typedoc": "rimraf docs && typedoc --name \"JayStack OData v4 Server\" --exclude \"**/?(utils|index).ts\" --excludeExternals --excludeNotExported --hideGenerator --excludePrivate --out docs src/lib/*.ts"
  },
  "watch": {
    "tsc": {
      "patterns": ["src"],
      "extensions": "ts",
      "quiet": false
    },
    "es6": {
      "patterns": ["src"],
      "extensions": "js",
      "quiet": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaystack/odata-v4-server.git"
  },
  "keywords": [
    "OData",
    "server",
    "V4",
    "server"
  ],
  "author": "JayStack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaystack/odata-v4-server/issues"
  },
  "homepage": "https://github.com/jaystack/odata-v4-server#readme",
  "dependencies": {
    "@types/body-parser": "0.0.33",
    "@types/cors": "0.0.33",
    "@types/express": "^4.0.33",
    "@types/extend": "^2.0.30",
    "@types/qs": "^6.2.30",
    "body-parser": "^1.15.2",
    "cors": "^2.8.0",
    "express": "^4.14.0",
    "extend": "^3.0.0",
    "odata-v4-literal": "^0.1.0",
    "odata-v4-metadata": "^0.1.3",
    "odata-v4-parser": "^0.1.18",
    "odata-v4-service-document": "0.0.3",
    "odata-v4-service-metadata": "^0.1.5",
    "qs": "^6.3.0",
    "reflect-metadata": "^0.1.8"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.30",
    "@types/event-stream": "^3.3.29",
    "@types/jsonstream": "^0.8.28",
    "@types/mocha": "^2.2.32",
    "@types/mongodb": "^2.1.33",
    "@types/node": "^7.0.18",
    "@types/node-uuid": "^0.0.28",
    "JSONStream": "^1.2.1",
    "benchmark": "^2.1.1",
    "chai": "^3.5.0",
    "copyfiles": "^1.2.0",
    "event-stream": "^3.3.4",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "mochawesome": "^1.5.3",
    "mongodb": "^2.2.6",
    "mssql": "^3.3.0",
    "npm-watch": "^0.1.6",
    "odata-v4-inmemory": "^0.1.7",
    "odata-v4-mongodb": "^0.1.5",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.4",
    "tslint": "^5.4.3",
    "typedoc": "^0.5.10",
    "typedoc-default-themes": "^0.4.0",
    "typedoc-plugin-external-module-name": "^1.0.5",
    "typescript": "^2.2.1"
  }
}
